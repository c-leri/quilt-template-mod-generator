{
	"name": "quilt-template-mod-generator",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"deploy": "vite build && gh-pages -d build -t true",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@iconify-json/fa6-brands": "^1.1.18",
		"@iconify-json/fa6-solid": "^1.1.20",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@sveltekit-i18n/base": "^1.3.7",
		"@sveltekit-i18n/parser-default": "^1.1.1",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"@unocss/extractor-svelte": "^0.58.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"gh-pages": "^6.1.1",
		"jszip": "^3.10.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"quilt-bulma": "^0.0.23",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.3",
		"svelte-markdown": "^0.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"unocss": "^0.58.3",
		"vite": "^5.0.11"
	},
	"type": "module"
}